<p id="notice"><%= notice %></p>

<h1>Listing Fabs</h1>

<table>
  <thead>
    <tr>
      <th>Users</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fabs.each do |fab| %>
      <tr>
        <td><%= fab.user_id %></td>
        <td><%= link_to 'Show', [@user, fab] %></td>
        <td><% link_to 'Edit', edit_user_fab_path(@user, fab) %></td>
        <td><%= link_to 'Destroy', [@user, fab], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fab', new_user_fab_path %>
